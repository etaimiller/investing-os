#!/usr/bin/env python3
"""
Investment OS CLI wrapper

Executable entrypoint for the investos command.
Calls the main CLI module from repo root.
"""

import sys
from pathlib import Path

# Add tools directory to path so we can import investos
repo_root = Path(__file__).parent.parent
sys.path.insert(0, str(repo_root))

from tools.investos.cli import main

if __name__ == '__main__':
    sys.exit(main())
